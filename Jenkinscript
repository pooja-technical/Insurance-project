pipeline {
   ageny any

tools { 
   maven 'M2_HOME' 
  }

stages {
  stage('Git Chekout')  {
    steps {
         echo 'Cloning Code from github'
     git 'https://github.com/pooja-technical/Insurance-project'
          }
     }

  stage('Code Build') {
       steps {
          echo 'Packaging the code'
      sh 'mvn install'
            }
     }
  }
}
